{
  "name": "@micro-os-plus/utils-lists",
  "version": "4.0.0-pre",
  "description": "A source code library with the ÂµOS++ C++ lists utilities",
  "main": "",
  "scripts": {
    "postversion": "git push origin --all && git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/micro-os-plus/utils-lists-xpack.git"
  },
  "homepage": "https://github.com/micro-os-plus/utils-lists-xpack",
  "bugs": {
    "url": "https://github.com/micro-os-plus/utils-lists-xpack/issues"
  },
  "keywords": [
    "xpack",
    "c++",
    "lists",
    "intrusive",
    "micro-os-plus"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "https://github.com/ilg-ul"
  },
  "license": "MIT",
  "config": {},
  "bundleDependencies": [],
  "$devDependenciesUrls": [
    "https://www.npmjs.com/package/del-cli"
  ],
  "devDependencies": {
    "del-cli": "^5.0.0",
    "open-cli": "^7.2.0"
  },
  "xpack": {
    "minimumXpmRequired": "0.16.2",
    "actions": {
      "deep-clean": "del-cli node_modules xpacks website/html package-lock.json",
      "install": [
        "npm install",
        "xpm install"
      ],
      "git-log": "git log --pretty='%cd * %h %s' --date=short",
      "npm-pack": "npm pack",
      "version-patch": "npm version patch",
      "version-minor": "npm version minor",
      "npm-version": "echo $(which node) $(node --version) && echo $(which npm) $(npm --version)",
      "doxygen": [
        "del-cli website/html",
        "doxygen website/config.doxyfile"
      ],
      "browser": "open-cli website/html/index.html"
    },
    "dependencies": {},
    "devDependencies": {
      "@jothepro/doxygen-awesome-css": "github:jothepro/doxygen-awesome-css#v2.2.1",
      "@micro-os-plus/build-helper": "github:micro-os-plus/build-helper-xpack#xpack-develop"
    },
    "properties": {},
    "buildConfigurations": {}
  }
}
